<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:background="@color/material_dynamic_primary10"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/homeLayout"
        android:layout_width="411dp"
        android:layout_height="244dp"
        android:background="@color/blue_100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.322"
        app:circularflow_radiusInDP="@string/hello_blank_fragment"
        tools:context=".WorkFragment">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Experienced Java Developer  Mary Ellis "
            android:shadowRadius="@integer/material_motion_duration_short_1"
            android:textColor="@color/red_300"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="-3dp"
            tools:layout_editor_absoluteY="113dp" />


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="97dp"
            android:layout_height="115dp"
            android:src="@mipmap/foto" />


        <!-- TODO: Update blank fragment layout -->

    </FrameLayout>




</androidx.constraintlayout.widget.ConstraintLayout>

















